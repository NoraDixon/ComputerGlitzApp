<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andriod.computerglitzapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Hands On"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Hands On">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Checklist"
            android:label="Hands On" />
        <activity
            android:name=".Favorites"
            android:label="Hands On" />
        <activity
            android:name=".SchoolInfo"
            android:label="Hands On" />
        <activity
            android:name=".TrendingJobs"
            android:label="Hands On" />
        <activity
            android:name=".CollegeListActivity"
            android:label="Hands On" />
        <activity
            android:name=".IndividsualChecklist"
            android:label="Hands On" />
        <activity
            android:name=".Notifications"
            android:label="Hands On"></activity>
        <activity
            android:name=".FinancailInfo"
            android:label="Hands On"></activity>
        <activity
            android:name=".Skills"
            android:label="Hands On"></activity>

        <receiver android:name=".AlarmReciver">
            <intent-filter>
                <action android:name="Action"></action>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>

    </application>

</manifest>